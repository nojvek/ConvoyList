{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/src/client/",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "verboseDiagnosticLogging": false
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": false,
            "webRoot": "${workspaceRoot}"
        }
    ]
}